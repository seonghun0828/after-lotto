{
    "semi": true,
    "tabWidth": 2,
    "printWidth": 100,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "arrowParens": "always",
    "embeddedLanguageFormatting": "auto",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ]
}